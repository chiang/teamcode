#!/bin/sh

REPOS="$1"
REV="$2"
USER="$3"
PROPNAME="$4"
ACTION="$5"

if [ "$ACTION" = "M" -a "$PROPNAME" = "svn:log" ]; then exit 0; fi
if [ "$PROPNAME" = "rms:id" ]; then exit 0; fi
if [ "$PROPNAME" = "tc:issuetracker:id" ]; then exit 0; fi

echo "Changing revision properties other than svn:log is prohibited" >&2
exit 1
